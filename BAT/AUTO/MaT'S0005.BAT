set ST_NO=ST010
set STATUS=0

call %BAT_PATH%\START10A.BAT

rem ===========================================================================
rem EXE実行
rem ---------------------------------------------------------------------------
call %EXE_PATH%\S0005.exe >> %TMPFILE%
if %errorlevel% == 0 (
    set STATUS=0
) else (
    set STATUS=9
)

call %BAT_PATH%\END0010A.BAT %STATUS%

if %STATUS%==9 goto END

rem ===========================================================================
rem Batch End 処理
