    Public Shared ReadOnly Property Instance() As CommonMessage
        Get
            '静的変数が解放されていた場合のみ、インスタンスを生成する
            If IsDBNull(_instance) Then
                _instance = New CommonMessage()
            End If
            Return _instance
        End Get
